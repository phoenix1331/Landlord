{
    "name": "phoenix1331/landlord",
    "description": "A simple, single database multi-tenancy solution for Laravel 7",
    "license": "MIT",
    "keywords": [
        "tenant",
        "tenancy",
        "multitenant",
        "multitenancy"
    ],
    "authors": [
        {
            "name": "Caleb Fidecaro",
            "email": "calebfidecaro@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2.5"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^8.5",
        "laravel/framework": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "HipsterJazzbo\\Landlord\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "HipsterJazzbo\\Landlord\\LandlordServiceProvider"
            ],
            "aliases": {
                "Landlord": "HipsterJazzbo\\Landlord\\Facades\\Landlord"
            }
        }
    }
}
